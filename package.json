{
	"name": "qsql-4-vscode",
	"description": "Oracle Quick SQL VS Code extension",
	"version": "0.0.2",
	"publisher": "vadimtro",
	"engines": {
		"vscode": "^1.74.0"
	},
	"activationEvents": [
		"onLanguage:qsql"
	],
	"main": "./extension.js",
	"contributes": {
		"languages": [
			{
				"id": "qsql",
				"extensions": [
					".qsql",
					".quicksql"
				]
			}
		],
		"grammars": [],
		"commands": [
			{
				"command": "extension.toDDL",
				"title": "Translate QSQL to DDL"
			},
			{
				"command": "extension.fromJSON",
				"title": "Translate JSON to QSQL"
			}
		]
	},
	"scripts": {},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Vadimtro/vscode-quicksql"
	},
	"devDependencies": {
		"@types/vscode": "^1.73.0"
	},
	"dependencies": {
		"@oracle/quicksql": "^1.2.12"
	}
}
